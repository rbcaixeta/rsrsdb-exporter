{
	"name": "RSRSDb Data Exporter",
	"description": "Romancing SaGa re:univerSe data extractor utility. Utilized for rsrsdb.com website.",
	"version": "1.0",
	"manifest_version": 2,
	"background": {
		"persistent": true,
		"scripts": ["background.js"]
	},
	"icons": {
		"16": "icon-192x192.png",
		"32": "icon-192x192.png",
		"48": "icon-192x192.png",
		"128": "icon-192x192.png"
	},
	"permissions": ["webRequest", "https://production-api.rs-us.aktsk.com/*"],
	"externally_connectable": {
		"matches": ["https://*.rsrsdb.com/*"]
	}
}
